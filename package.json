{
  "name": "safecmd",
  "version": "0.0.2",
  "description": "SAFE Network command line application (CLI) boilerplate example",
  "main": "safecmd.js",
  "scripts": {
    "testbuild": "pkg . -d -t node8-linux-x64 -o ./dist/mock/safecmd",
    "build": "mkdir -p ./dist/prod/node_modules/@maidsafe/safe-node-app/src/native/prod/ && cp -R node_modules/@maidsafe/safe-node-app/src/native/prod/*.* ./dist/prod/node_modules/@maidsafe/safe-node-app/src/native/prod/ && pkg -t host -o ./dist/prod/safecmd safecmd.js",
    "buildall": "mkdir -p ./dist/prod/node_modules/@maidsafe/safe-node-app/src/native/prod/ && cp -R node_modules/@maidsafe/safe-node-app/src/native/prod/*.* ./dist/prod/node_modules/@maidsafe/safe-node-app/src/native/prod/ && pkg -t linux,macos,win -o ./dist/prod/safecmd safecmd.js",
    "build-mock": "mkdir -p ./dist/mock/node_modules/@maidsafe/safe-node-app/src/native/mock/ && cp -R node_modules/@maidsafe/safe-node-app/src/native/mock/*.* ./dist/mock/node_modules/@maidsafe/safe-node-app/src/native/mock/ && pkg -t host -o ./dist/mock/safecmd safecmd.js",
    "buildall-mock": "mkdir -p ./dist/mock/node_modules/@maidsafe/safe-node-app/src/native/mock/ && cp -R node_modules/@maidsafe/safe-node-app/src/native/mock/*.* ./dist/mock/node_modules/@maidsafe/safe-node-app/src/native/mock/ && pkg -t linux,macos,win -o ./dist/mock/safecmd safecmd.js"
  },
  "pkg": {
    "assets": [
      "node_modules/@maidsafe/safe-node-app/src/native/prod/*.*",
      "node_modules/@maidsafe/safe-node-app/src/native/mock/*.*"
    ]
  },
  "bin": {
    "app": "safecmd.js"
  },
  "author": "theWebalyst",
  "license": "GPL-3.0",
  "repository": "https://github.com/theWebalyst/safe-cli-boilerplate",
  "dependencies": {
    "@maidsafe/safe-node-app": "^0.8.1",
    "filesize": "^3.6.1",
    "node-ipc": "^9.1.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "pkg": "^4.3.1"
  }
}
